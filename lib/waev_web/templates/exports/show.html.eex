ID: <%= @id %>

<section class="phx-hero">
  <section class="row">
    <article class="column">
      <h2><%= @export.left %></h2>
    </article>
    <article class="column">
      <h2><%= @export.right %></h2>
    </article>
  </section>
  <%= for message <- @export.messages do %>
    <section class="row">
      <article class="column">
        <%= message.text %>
      </article>
      <article class="column">
      </article>
    </section>
  <% end %>
</section>
