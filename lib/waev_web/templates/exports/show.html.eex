ID: <%= @id %>

<div class="container">
  <div class="row">
    <div class="column column-50">
      <%= @export.left %>
    </div>
    <div class="column column-50">
      <%= @export.right %>
    </div>
  </div>
  <%= for message <- @export.messages do %>
    <div class="row">
      <div class="column column-75">
        <%= message.text %>
        <br />
        <%= message.date %>
      </div>
    </div>
  <% end %>
</div>
